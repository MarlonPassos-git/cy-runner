{
  "name": "cypress-conductor",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,graphql,gql}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/node": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@vtex/prettier-config": "^1.0.0",
    "@vtex/tsconfig": "^0.6.0",
    "cypress": "^9.4.1",
    "eslint": "^8.9.0",
    "eslint-config-vtex": "^14.1.2",
    "eslint-plugin-jest": "^26.1.0",
    "husky": "^7.0.4",
    "js-yaml": "^4.1.0",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  }
}
